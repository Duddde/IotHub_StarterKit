<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>
		<aspNetCore processPath="dotnet" arguments=".\Coneo.Api.dll" stdoutLogEnabled="false" stdoutLogFile="\\?\%home%\LogFiles\stdout" hostingModel="inprocess" />
		<httpErrors errorMode="Detailed"></httpErrors>
		<asp scriptErrorSentToBrowser="true"/>
	</system.webServer>
	<system.web>
		<customErrors mode="Off"/>
		<compilation debug="true"/>
	</system.web>
</configuration>